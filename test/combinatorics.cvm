

int brutal(int n, int m) {
    int sum, i, j;
	if (n < 1 || m < 1) return 0;
	iput(n);
	iput(m);
	i = 1;
	while (i <= n) {
		j = 1;
		while (j <= m) {
		    sum = sum + i * j;
		    iput(sum);
			j = j + 1;
		}		
		i = i + 1;
	}
	return sum;
}


int main() {	
    int x;
	x = brutal(5,5);
	return x;
}
